.loc 0xA00

main:
    llis    ra, 0xFFFF
    reloc   ra, 0       // place stack at 0xFFFF
    llis    ra, 0

loop:
    add     ra, ra, 1
    add     rb, rb, 1
    add     rc, rc, 1
    jal     GPU_set_cursor_color
    jal     GPU_set_cursor_pos
    bra     loop

.inc "asm/gpu.aphel"


